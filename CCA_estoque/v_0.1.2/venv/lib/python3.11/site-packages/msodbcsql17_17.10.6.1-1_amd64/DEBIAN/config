#!/bin/bash -e

# Source debconf library.
. /usr/share/debconf/confmodule


# EULA acceptance external (for suppression)
db_get msodbcsql/ACCEPT_EULA
if [ "$RET" != true ] && [ "$ACCEPT_EULA" != y ] && [ "$ACCEPT_EULA" != Y ] ; then
    set +e
    db_input high msodbcsql/ACCEPT_EULA
    db_go
    set -e
fi
